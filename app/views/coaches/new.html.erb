<h1>coach/new</h1>
<% if @coach.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@coach.errors.count, "error") %>
      prohibited this account from being created:
    </h2>
 
    <ul>
    <% @coach.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= render 'form' %>