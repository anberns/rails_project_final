<h1>Update Account</h1><br>
<% if @swimmer.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@swimmer.errors.count, "error") %>
      prohibited this account from being updated:
    </h2>
 
    <ul>
    <% @swimmer.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<%= render 'form' %>