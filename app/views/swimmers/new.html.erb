<h1>swimmer/new</h1>
<h1>Sign Up</h1>
<% if @swimmer.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@swimmer.errors.count, "error") %>
      prohibited this account from being created:
    </h2>
 
    <ul>
    <% @swimmer.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= render 'form' %>

