<h2><%= link_to "Add New Event", new_event_path %></h2>
<h3>Filter Events</h3>
<%= form_tag "/events/filter", method: 'post' do %>
  <%= select_tag "filter_type", options_for_select(["None", "Distance", "Stroke"]) %>
  <input type="text" name="filter_value"><br>
  <input type="submit" value="Filter">
<% end %>
  
<% @events.each do |event| %>
  <p><%= event.name %><p>
  <%= link_to "Edit", edit_event_path(event.id) %>
  <%= form_tag event_path(event.id), method: 'delete' do %>
    <input class="button delete" type="submit" id="submit" value="Delete"><br>
  <% end %>
<% end %>
