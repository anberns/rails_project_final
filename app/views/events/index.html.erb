<% @events.each do |event| %>
  <p><%= event.name %><p>
  <%= link_to "Edit", edit_event_path(event.id) %>
  <%= form_tag event_path(event.id) do %>
    <input type="hidden" name="_method" value="delete" />
    <input class="button delete" type="submit" id="submit" value="Delete"><br>
  <% end %>
<% end %>
